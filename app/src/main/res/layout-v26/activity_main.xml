<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="@string/password"
            android:ems="10"
            android:id="@+id/editText7" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="343dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="99dp" android:layout_marginStart="99dp" android:autofillHints=""/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="@string/email"
            android:ems="10"
            android:id="@+id/editText8" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="297dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="98dp" android:layout_marginStart="98dp" android:autofillHints=""/>
    <Button
            android:text="@string/session_close"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/close_session" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="388dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="141dp" android:layout_marginStart="141dp"
            android:onClick="sendMessage"/>
</android.support.constraint.ConstraintLayout>